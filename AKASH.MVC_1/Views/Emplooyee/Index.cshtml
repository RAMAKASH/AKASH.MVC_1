@model IEnumerable<AKASH.MVC_1.Models.Employee>
@{
}
<a asp-action="Create" Asp-Controller="Employee" class="btn btn-outline-danger">Create</a>
<table class="table table-bordered">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Email</th>
            <th>Mobile</th>
            <th>Address</th>
            <th>Action</th>
        </tr>
    </thead>
    @foreach(var emp in Model) 
    {
<tr>
    <td> @emp.Id</td>
    <td> @emp.Name</td>
    <td> @emp.Email</td>
    <td> @emp.Mobile</td>
    <td> @emp.Address</td>

    <td>
        <a asp-action="Edit" asp-controller="Employee" asp-route-id="@emp.Id"
           onclick="return confirm('Are you sure to delete this record'?)">Edit</a>
        <a asp-action="Delete"  asp-controller="Employee" asp-router-id="@emp.Id"
           onclick="return confirm('Are you sure to delete this record'?)">Delete </a>

    </td>

</tr>
    }
</table>